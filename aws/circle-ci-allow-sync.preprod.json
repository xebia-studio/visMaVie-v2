{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CirleCIAllowSyncBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::xebia-fr-2016-preprod"
            ]
        },
        {
            "Sid": "CirleCIAllowSyncObjects",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObject",
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::xebia-fr-2016-preprod/*"
            ]
        }
    ]
}