{
    "Version": "2017-01-17",
    "Statement": [
        {
            "Sid": "CirleCIAllowSyncBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::vis-ma-vie-preprod"
            ]
        },
        {
            "Sid": "CirleCIAllowSyncObjects",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObject",
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::vis-ma-vie-preprod/*"
            ]
        }
    ]
}