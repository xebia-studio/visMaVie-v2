{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CirleCIAllowSyncBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:PutBucketPolicy"
            ],
            "Resource": [
                "arn:aws:s3:::xebia-vis-ma-vie--master"
            ]
        },
        {
            "Sid": "CirleCIAllowSyncObjects",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObject",
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::xebia-vis-ma-vie--master/*"
            ]
        }
    ]
}