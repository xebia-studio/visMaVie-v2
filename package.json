  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "webpack -d",
    "build:prod": "webpack -p",

    	"prestart": "npm install",
    "start": "export NODE_ENV=development && webpack-dev-server --port=9000 --content-base app/",

    "phantom": "phantomjs --webdriver=4444",
    "codecept": "codeceptjs run --steps",
    "test": "concurrently \"npm run phantom\" \"npm run codecept\" ",

		"deploy" : "npm run deploy:dev",
		"deploy:dev": "aws s3 sync ./dist s3://xebia-vis-ma-vie--dev --profile xebia",
		"predeploy:master": "npm run build",
		"deploy:master": "aws s3 sync ./dist s3://xebia-vis-ma-vie--master --profile xebia",
		"deploy:integ": "aws s3 sync ./dist s3://xebia-vis-ma-vie--integ --profile xebia"
	},
